{
  "name": "@dreamer/api",
  "version": "0.1.0",
  "description": "Backend API server for Dream Analyzer",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@dreamer/shared": "workspace:*",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.10.2",
    "fastify": "^4.26.2",
    "pino-pretty": "^13.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "prisma": "^5.10.2",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^3.2.4"
  }
}